<persistence xmlns="http://java.sun.com/xml/ns/persistence"
             xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
             xsi:schemaLocation="http://java.sun.com/xml/ns/persistence http://java.sun.com/xml/ns/persistence/persistence_2_0.xsd"
             version="2.0">
	<persistence-unit name="yoyodyne.foo.persistence">
		<provider>org.hibernate.jpa.HibernatePersistenceProvider</provider>
		<class>yoyodyne.foo.db.Table1</class>
		<class>yoyodyne.foo.db.BusinessUnit</class>
		<class>yoyodyne.foo.db.Function</class>
		<class>yoyodyne.foo.db.FunctionType</class>
		<class>yoyodyne.foo.db.MetricType</class>
		<class>yoyodyne.foo.db.User</class>
	</persistence-unit>
</persistence>